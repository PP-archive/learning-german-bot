{
  "name": "learning-german-bot",
  "version": "1.0.0",
  "description": "LearningGermanBot on telegram",
  "repository": {
    "type": "git",
    "url": "git://github.com:PavelPolyakov/learning-german-bot.git"
  },
  "main": "index.js",
  "scripts": {
    "ngrok": "ngrok http -subdomain=lgb-dev 3000",
    "app": "DEBUG=* node --harmony --harmony_destructuring index.js",
    "test": "lab ./test/ --pattern \"^[^_]\" --verbose",
    "test-unit": "lab ./test/unit --pattern \"^[^_]\" --verbose",
    "test-integration": "lab ./test/integration --pattern \"^[^_]\" --verbose",
    "test-coverage": "lab ./test/ --pattern \"^[^_]\" --reporter html --output ./test/coverage.html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.3.4",
    "botanio": "0.0.6",
    "code": "^2.1.1",
    "config": "^1.19.0",
    "cron": "^1.1.0",
    "debug": "^2.2.0",
    "glue": "^3.2.0",
    "hapi": "^13.2.1",
    "js-yaml": "^3.5.5",
    "lab": "^10.3.0",
    "levenshtein": "^1.0.5",
    "local-modules": "^1.0.1",
    "lodash": "^4.6.1",
    "moment": "^2.12.0",
    "mongoose": "^4.4.10",
    "node-emoji": "^1.2.1",
    "node-telegram-bot-api": "^0.21.1"
  }
