{
  "name": "learning-german-bot",
  "version": "1.0.0",
  "description": "LearningGermanBot on telegram",
  "repository": {
    "type": "git",
    "url": "git://github.com:PavelPolyakov/learning-german-bot.git"
  },
  "main": "index.js",
  "scripts": {
    "ngrok": "ngrok http -subdomain=lgb-dev 3000",
    "app": "DEBUG=* node --harmony index.js",
    "cli": "DEBUG=* node --harmony cli.js",
    "test-unit": "node $NODE_DEBUG_OPTION --expose_debug_as=v8debug ./node_modules/lab/bin/lab ./test/unit --pattern \"^[^_]\" --verbose",
    "test-integration": "DEBUG=* node $NODE_DEBUG_OPTION --expose_debug_as=v8debug ./node_modules/lab/bin/lab ./test/integration --pattern \"^[^_]\" --verbose",
    "test-coverage": "lab ./test/ --pattern \"^[^_]\" --reporter html --output ./test/coverage.html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.0",
    "botanio": "0.0.6",
    "code": "^3.0.0",
    "config": "^1.20.4",
    "cron": "^1.1.0",
    "debug": "^2.2.0",
    "glue": "^3.2.1",
    "hapi": "^13.4.1",
    "i18n": "^0.8.2",
    "js-yaml": "^3.6.1",
    "lab": "^10.7.1",
    "levenshtein": "^1.0.5",
    "local-modules": "^1.0.1",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "mongoose": "^4.4.19",
    "node-emoji": "^1.3.0",
    "node-telegram-bot-api": "^0.21.1",
    "parsec": "^1.5.3"
  }
}
